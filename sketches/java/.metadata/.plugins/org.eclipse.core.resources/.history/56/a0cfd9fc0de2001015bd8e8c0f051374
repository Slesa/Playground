
public class FizzBuzz {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int max = 10;
		for(int i=0; i<max; i++)
		{
			if( i%3==0 )
			{
				System.out.print("Fizz");
				continue;
			}
			if( i%5==0 )
			{
				System.out.print("Buzz");
				continue;
			}
			
		}
	}

}
