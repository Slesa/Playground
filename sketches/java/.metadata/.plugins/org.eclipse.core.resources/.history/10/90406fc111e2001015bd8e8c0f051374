import static org.junit.Assert.assertEquals;

import org.junit.Test;


public class When_investigating_3 extends FizzBuzzTest {

	@Test
	public void it_should_return_fizz() {
		String result = _fizzBuzz.get_output(3);
		assertEquals(result, "Fizz");
	}
}

public class When_investigating_5 extends FizzBuzzTest {
	
	@Test
	public void it_should_return_buzz() {
		String result = _fizzBuzz.get_output(5);
		assertEquals(result, "Buzz");
	}
}