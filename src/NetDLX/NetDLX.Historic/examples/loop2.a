        INCLUDE "start.i"

        LI      R1,#'!

Loop    OUTPUTBYTE
        ADDUI   r1,R1,#1
        SEQI    R2,R1,#'{
        BEQZ    R2,Loop

        LI      R1,#$A
        OUTPUTBYTE

        HALT

        end


